{
  "include": [
    "./src/*"
  ],
  "compilerOptions": {
    "lib": [
      "ES2015",
    ],
    "moduleResolution": "node16",
    "module": "Node16",
    "outDir": "./dist",
    "strict": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "newLine": "lf"
  }
}